{
  "manifest_version": 2,

  "name": "Disposable",
  "description": "One-click Reddit throwaway generator",
  "version": "1.0.0",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html",
    "default_title": "Create Reddit throwaway"
  },
  "permissions": [
    "https://www.reddit.com/"
  ]
}
